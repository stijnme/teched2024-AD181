{
  "name": "customer_loyalty_211",
  "version": "1.0.0",
  "description": "Customer Loyalty Program Model and Services",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-cloud-sdk/http-client": "^3.22.2",
    "@sap-cloud-sdk/util": "^3.22.2",
    "@sap/cds": "^8",
    "@sap/low-code-event-handler": "^1.2.4",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^4.2.7",
    "express": "^4",
    "hdb": "^0.19.10",
    "passport": "^0.7.0",
    "@sap/mdk-tools": "^1.3.1"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.7.0",
    "@cap-js/sqlite": "^1",
    "@sap/dev-cap-tools": "^1.31.0"
  },
  "scripts": {
    "start": "cds-serve",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open",
    "watch-customer_loyalty_211.purchases": "cds watch --open customer_loyalty_211.purchases/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-customer_loyalty_211.customers": "cds watch --open customer_loyalty_211.customers/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-customer_loyalty_211.redemptions": "cds watch --open customer_loyalty_211.redemptions/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "app-service": {
        "impl": "@sap/low-code-event-handler"
      },
      "auth": {
        "[development]": {
          "kind": "dummy"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      },
      "S4HANA_Joule_Product": {
        "kind": "odata-v2",
        "model": "srv/external/S4HANA_Joule_Product",
        "[production]": {
          "credentials": {
            "destination": "S4HANA_Joule_Product"
          }
        },
        "csrf": true,
        "csrfInBatch": true
      }
    }
  },
  "lcap": true,
  "sapux": [
    "app/customer_loyalty_211.purchases",
    "app/customer_loyalty_211.customers",
    "app/customer_loyalty_211.redemptions"
  ]
}
